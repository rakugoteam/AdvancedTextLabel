; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[addons]

rakugo/game_version="1.0.0"
rakugo/history_length=30
rakugo/narrator/name="narrator"
rakugo/debug=false
rakugo/save_folder="user://saves"

[application]

config/name="AdvancedText"
config/tags=PackedStringArray("addon", "rakugo")
run/main_scene="res://addons/advanced-text/examples/advanced_text_labels.tscn"
config/features=PackedStringArray("4.2")
config/icon="res://addons/advanced-text/icons/AdvancedTextLabel.svg"

[autoload]

Rakugo="*res://addons/Rakugo/Rakugo.gd"
MaterialIconsDB="*res://addons/material-design-icons/icons/icons.gd"
EmojisDB="*res://addons/emojis-for-godot/emojis/emojis.gd"

[debug]

gdscript/warnings/unassigned_variable=false
gdscript/warnings/unassigned_variable_op_assign=false
gdscript/warnings/unused_variable=false
gdscript/warnings/return_value_discarded=false
gdscript/completion/autocomplete_setters_and_getters=true
gdscript/warnings/unused_argument=false

[editor]

script/search_in_file_extensions=PackedStringArray("gd", "gdshader", "rk")

[editor_plugins]

enabled=PackedStringArray("res://addons/Rakugo/plugin.cfg", "res://addons/advanced-text/plugin.cfg", "res://addons/emojis-for-godot/plugin.cfg", "res://addons/gut/plugin.cfg", "res://addons/material-design-icons/plugin.cfg", "res://addons/nv.file_system/plugin.cfg", "res://addons/script-ide/plugin.cfg")

[global]

debug=false

[physics]

common/enable_pause_aware_picking=true

[rendering]

environment/defaults/default_environment="res://default_env.tres"
